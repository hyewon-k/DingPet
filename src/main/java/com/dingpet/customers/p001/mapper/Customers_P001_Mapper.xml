<mapper namespace="com.dingpet.customers.p001.mapper.Customers_P001_Mapper">

<!-- 일반회원 등록 -->
<select id="insertmember">
	insert into member(member_id, member_pwd, member_nickname, member_email)
	values(#{member_id}, #{member_pwd}, #{member_nickname}, #{member_email})
</select>






